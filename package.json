{
  "name": "cloud-pine",
  "version": "1.0.0",
  "description": "Pino Transport abstraction for Google Cloud Logging.",
  "main": "index.js",
  "bin": {
    "cloud-pine": "./cli.js"
  },
  "types": "index.d.ts",
  "scripts": {
    "test": "tap --cov test/**/*.test.js && npm run test:ts && npm run lint",
    "test:ci": "tap --cov test/**/*.test.js && npm run test:ts && npm run lint:ci",
    "test:only": "tap --only",
    "test:unit": "tap test/**/*.test.js",
    "test:ts": "tsd",
    "lint": "standard | snazzy",
    "lint:ci": "standard",
    "release": "npx standard-version"
  },
  "keywords": [
    "pino",
    "pine",
    "gcp",
    "google-cloud",
    "google-cloud-platform",
    "cloud",
    "cloud-pine",
    "logging",
    "cloud-logging"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metcoder95/cloud-pine.git"
  },
  "readme": "https://github.com/metcoder95/cloud-pine/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/metcoder95/cloud-pine/issues"
  },
  "author": "metcoder95 <me@metcoder.dev>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "husky": "^8.0.1",
    "pino": "^8.2.0",
    "snazzy": "^9.0.0",
    "standard": "^17.0.0",
    "tap": "^16.3.0",
    "tsd": "^0.28.0",
    "typescript": "^5.0"
  },
  "dependencies": {
    "@google-cloud/logging": "^10.1.1",
    "minimist": "^1.2.6",
    "split2": "^4.1.0"
  },
  "tsd": {
    "directory": "test"
  },
  "tap": {
    "check-coverage": false
  },
  "standard": {
    "ignore": [
      "*.d.ts",
      "*.test-d.ts"
    ]
  }
}
